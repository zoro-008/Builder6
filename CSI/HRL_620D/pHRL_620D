INCLUDEPATH = "Motion\새 폴더";Motion;Vision;Include\Imi;SMDll;"C:\Program Files\Borland\CBuilder6\Projects";$(BCB)\include;$(BCB)\include\vcl;SMDll\include\AXL;SMDll\include\AXT;include\Hanra;include\Euresys
LIBPATH = "Motion\새 폴더";Motion;Vision;Include\Imi;SVDll;SMDll;"C:\Program Files\Borland\CBuilder6\Projects";D:\BC6Works\HRD_210c;$(BCB)\lib\obj;$(BCB)\lib;SMDll\include\AXT;SMDll\include\AXL;SMDll\include\AxtLib
WARNINGS= -w-par
PATHCPP = .;SMDll;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion;Motion
