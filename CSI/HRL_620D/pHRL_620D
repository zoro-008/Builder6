RESDEPEN = $(RESFILES) Motion\FormDevice.dfm Motion\FormDeviceSet.dfm \
    Motion\FormInform.dfm Motion\FormLoading.dfm Motion\FormMain.dfm \
    Motion\FormOperation.dfm Motion\FormMaster.dfm Motion\FrameMotr.dfm
LIBFILES = include\Hanra\SLDll.lib
IDLFILES = 
